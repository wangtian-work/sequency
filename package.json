{
  "name": "sequency",
  "version": "0.6.0",
  "description": "Functional sequences for processing iterable data in JavaScript",
  "main": "lib/Sequence.js",
  "umd:main": "lib-umd/sequence.js",
  "typings": "lib/Sequence.d.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch --notify",
    "test:coverage": "jest --coverage",
    "docs": "typedoc --name Sequency --readme APIDOC.md --module commonjs --out docs --target es6 --hideGenerator --gaID UA-48569937-1 src",
    "clean": "rimraf lib && rimraf lib-umd && rimraf docs && rimraf coverage",
    "prebuild": "yarn run clean",
    "build": "yarn test && tsc && webpack",
    "prepublishOnly": "yarn run build"
  },
  "author": "Benjamin Winterberg",
  "homepage": "http://winterbe.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/winterbe/sequency.git"
  },
  "bugs": {
    "url": "https://github.com/winterbe/sequency/issues"
  },
  "files": [
    "lib",
    "lib-umd",
    "LICENSE"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "devDependencies": {
    "@types/jest": "^20.0.7",
    "awesome-typescript-loader": "^3.2.3",
    "jest": "^20.0.4",
    "rimraf": "^2.6.1",
    "typedoc": "^0.8.0",
    "typescript": "^2.4.2",
    "webpack": "^3.5.4"
  },
  "dependencies": {},
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/test/*.ts"
    ]
  },
  "keywords": [
    "functional",
    "sequence",
    "processing",
    "lazy",
    "iterable",
    "iterator",
    "array",
    "map",
    "set"
  ]
}
